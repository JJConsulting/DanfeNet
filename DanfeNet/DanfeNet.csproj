<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <LangVersion>12</LangVersion>
    <TargetFramework>netstandard2.0</TargetFramework>
    <OutputType>Library</OutputType>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>DanfeNet</RootNamespace>
    <PackageId>DanfeNet</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.0</Version>
    <PackageVersion>$(Version)</PackageVersion>
    <AssemblyVersion>$(Version)</AssemblyVersion>
    <PackageId>DanfeNet</PackageId>
    <Copyright>© 2024 JJConsulting. All rights reserved.</Copyright>
    <AssemblyName>JJConsulting.DanfeNet</AssemblyName>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="PdfClown.NetStandard" Version="0.2.0-beta" />
    <PackageReference Include="QRCoder" Version="1.4.3" />
    <InternalsVisible Include="DanfeNet.Tests" />
  </ItemGroup>

</Project>
